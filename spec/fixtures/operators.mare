:describe operators
  :demo all // in order of precedence, from "weakest" to "strongest"
    y = x
    && x || x
    === x == x !== x != x =~ x
    >= x <= x < x > x
    <|> x <~> x <<< x >>> x <<~ x ~>> x << x >> x <~ x ~> x
    .. x <> x
    + x - x
    * x / x x.y
  
  :demo mixed
    a != b && c > d / x + e / y || i..j > k << l
  
  :demo prefix
    ~x
