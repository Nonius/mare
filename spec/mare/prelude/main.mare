:import "spec"

:actor Main
  :new (env)
    Specs.run([
      SpecRunner(ArraySpec).new(env)
      SpecRunner(BoolSpec).new(env)
      SpecRunner(CPointerSpec).new(env)
      SpecRunner(InspectSpec).new(env)
      SpecRunner(NumericSpec).new(env)
      SpecRunner(PlatformSpec).new(env)
      SpecRunner(StringSpec).new(env)
    ])
