:ffi LibC
  :fun printf (format CPointer(U8), arg1 CPointer(U8)) I32

:actor Greeter
  :new (message1 String, message2 String)
    LibC.printf("%s".cstring, message1.cstring)
    LibC.printf("%s".cstring, message2.cstring)

:actor Main
  :new (env)
    Greeter.new("What goes\n", "up\n")
    Greeter.new("Must come\n", "down\n")
    Greeter.new("Mare can\n", "now\n")
    Greeter.new("Schedule a\n", "cown\n")
